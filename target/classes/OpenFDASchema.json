{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Welcome8",
    "definitions": {
        "Welcome8": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "meta": {
                    "$ref": "#/definitions/Meta"
                },
                "results": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Result"
                    }
                }
            },
            "required": [
                "meta",
                "results"
            ],
            "title": "Welcome8"
        },
        "Meta": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "disclaimer": {
                    "type": "string"
                },
                "terms": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "license": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "last_updated": {
                    "type": "string",
                    "format": "date"
                },
                "results": {
                    "$ref": "#/definitions/Results"
                }
            },
            "required": [
                "disclaimer",
                "last_updated",
                "license",
                "results",
                "terms"
            ],
            "title": "Meta"
        },
        "Results": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "skip": {
                    "type": "integer"
                },
                "limit": {
                    "type": "integer"
                },
                "total": {
                    "type": "integer"
                }
            },
            "required": [
                "limit",
                "skip",
                "total"
            ],
            "title": "Results"
        },
        "Result": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "country": {
                    "$ref": "#/definitions/Country"
                },
                "city": {
                    "type": "string"
                },
                "address_1": {
                    "type": "string"
                },
                "reason_for_recall": {
                    "type": "string"
                },
                "address_2": {
                    "type": "string"
                },
                "product_quantity": {
                    "type": "string"
                },
                "code_info": {
                    "type": "string"
                },
                "center_classification_date": {
                    "type": "string",
                    "format": "integer"
                },
                "distribution_pattern": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "product_description": {
                    "type": "string"
                },
                "report_date": {
                    "type": "string",
                    "format": "integer"
                },
                "classification": {
                    "$ref": "#/definitions/Classification"
                },
                "openfda": {
                    "$ref": "#/definitions/Openfda"
                },
                "recalling_firm": {
                    "type": "string"
                },
                "recall_number": {
                    "type": "string"
                },
                "initial_firm_notification": {
                    "type": "string"
                },
                "product_type": {
                    "$ref": "#/definitions/ProductType"
                },
                "event_id": {
                    "type": "string",
                    "format": "integer"
                },
                "more_code_info": {
                    "type": "string"
                },
                "recall_initiation_date": {
                    "type": "string",
                    "format": "integer"
                },
                "postal_code": {
                    "type": "string"
                },
                "voluntary_mandated": {
                    "$ref": "#/definitions/VoluntaryMandated"
                },
                "status": {
                    "$ref": "#/definitions/Status"
                },
                "termination_date": {
                    "type": "string",
                    "format": "integer"
                }
            },
            "required": [
                "address_1",
                "address_2",
                "center_classification_date",
                "city",
                "classification",
                "code_info",
                "country",
                "distribution_pattern",
                "event_id",
                "initial_firm_notification",
                "openfda",
                "postal_code",
                "product_description",
                "product_quantity",
                "product_type",
                "reason_for_recall",
                "recall_initiation_date",
                "recall_number",
                "recalling_firm",
                "report_date",
                "state",
                "status",
                "voluntary_mandated"
            ],
            "title": "Result"
        },
        "Openfda": {
            "type": "object",
            "additionalProperties": false,
            "title": "Openfda"
        },
        "Classification": {
            "type": "string",
            "enum": [
                "Class II",
                "Class I",
                "Class III"
            ],
            "title": "Classification"
        },
        "Country": {
            "type": "string",
            "enum": [
                "United States"
            ],
            "title": "Country"
        },
        "ProductType": {
            "type": "string",
            "enum": [
                "Food"
            ],
            "title": "ProductType"
        },
        "Status": {
            "type": "string",
            "enum": [
                "Ongoing",
                "Terminated"
            ],
            "title": "Status"
        },
        "VoluntaryMandated": {
            "type": "string",
            "enum": [
                "Voluntary: Firm initiated"
            ],
            "title": "VoluntaryMandated"
        }
    }
}
